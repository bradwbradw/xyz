{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {
      "enabled":false
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../dist"
    }
  },
  "final": {
    "./middleware/serve-index-html": {

    }
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
         "debug": false,
         "log": true
       }
    }
  }
}
