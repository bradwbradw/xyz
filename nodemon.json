{
  "restartable": "rs",
  "ignore": [
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "api-test",
    "common",
    "server",
    "constants.js",
    "config.js",
    "nodemon.json",
    "start.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}